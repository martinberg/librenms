        rcEponOnuCtcDevCapSleepMode             INTEGER,
        rcEponOnuCtcDevCapEarlyWakeup           INTEGER
     }

    rcEponOnuCtcDevCapServiceType OBJECT-TYPE
        SYNTAX BITS{
               ge_bit(0),
               fe_bit(1),
               voip_bit(2),
               tdm_ces_bit(3),
               catv_bit(7)
               }
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Services types supported by the ONU."
        ::= { rcEponOnuCtcDevCapEntry 1 }

    rcEponOnuCtcDevCapGEPortNum OBJECT-TYPE
        SYNTAX INTEGER (0..256)
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Gigabit Ethernet ports number supported by the ONU."
        ::= { rcEponOnuCtcDevCapEntry 2 }

    rcEponOnuCtcDevCapGEPortBitMap OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..8))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "Gigabit Ethernet UNI interface distribution.
            Bit0~bit31 is GE port 1~32:
             1 - GE port
             0 £­not GE port"
