--
--  rcEponOnuCtcDevInfoTable
--

    rcEponOnuCtcDevInfoTable OBJECT-TYPE
        SYNTAX SEQUENCE OF  RcEponOnuCtcDevInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "."
        ::= { rcEponOnuDevGroup 1 }

     rcEponOnuCtcDevInfoEntry OBJECT-TYPE
        SYNTAX RcEponOnuCtcDevInfoEntry
        MAX-ACCESS not-accessible
        STATUS current
        DESCRIPTION
            "."
       INDEX { rcEponOnuDevIndex }
        ::= { rcEponOnuCtcDevInfoTable 1 }

    RcEponOnuCtcDevInfoEntry ::= SEQUENCE {
        rcEponOnuDevIndex                   OnuIndex,
        rcEponOnuCtcDevInfoVerdorId         OCTET STRING,
        rcEponOnuCtcDevInfoModel            PONDeviceType,
        rcEponOnuCtcDevInfoMacAddress       MacAddress,
        rcEponOnuCtcDevInfoHwVersion        OCTET STRING,
        rcEponOnuCtcDevInfoSwVersion        OCTET STRING,
        rcEponOnuCtcDevInfoFwVersion        OCTET STRING,
        rcEponOnuCtcDevInfoPonChipVendorId  OCTET STRING,
        rcEponOnuCtcDevInfoPonChipModel     OCTET STRING,
        rcEponOnuCtcDevInfoPonChipRevision  OCTET STRING,
        rcEponOnuCtcDevInfoPonChipVersion   OCTET STRING,
        rcEponOnuCtcDevInfoName             OCTET STRING,
        rcEponOnuCtcDevInfoPhysicalSerialNum  OCTET STRING
     }

    rcEponOnuDevIndex OBJECT-TYPE
        SYNTAX OnuIndex
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "."
