    rcEponOnuCtcDevInfoModel OBJECT-TYPE
        SYNTAX PONDeviceType
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The model of the ONU."
        ::= { rcEponOnuCtcDevInfoEntry 3 }

    rcEponOnuCtcDevInfoMacAddress OBJECT-TYPE
        SYNTAX MacAddress
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "This value indicates the identifier of the ONU."
        ::= { rcEponOnuCtcDevInfoEntry 4 }

    rcEponOnuCtcDevInfoHwVersion OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..8))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The hardware version of ONU."
        ::= { rcEponOnuCtcDevInfoEntry 5 }

    rcEponOnuCtcDevInfoSwVersion OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..16))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The software version of ONU."
        ::= { rcEponOnuCtcDevInfoEntry 6 }

    rcEponOnuCtcDevInfoFwVersion OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..16))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The firmware version of ONU."
        ::= { rcEponOnuCtcDevInfoEntry 7 }

    rcEponOnuCtcDevInfoPonChipVendorId OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..4))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The verdorId of pon chip."
        ::= { rcEponOnuCtcDevInfoEntry 8 }

    rcEponOnuCtcDevInfoPonChipModel OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..8))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The model of pon chip."
        ::= { rcEponOnuCtcDevInfoEntry 9 }

    rcEponOnuCtcDevInfoPonChipRevision OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..8))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The revision information of pon chip."
        ::= { rcEponOnuCtcDevInfoEntry 10 }

    rcEponOnuCtcDevInfoPonChipVersion OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..16))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The version information of pon chip.At present,the chip design date is only supported by PAS SOFT.
            And the chip design date represented in three bytes array in the format of [YY,MM,DD],
            where Y,M,D are numbers. For example: Oct 22, 2006 would be represented as [06,10,22]."
        ::= { rcEponOnuCtcDevInfoEntry 11 }  
        
     rcEponOnuCtcDevInfoName OBJECT-TYPE
        SYNTAX OCTET STRING (SIZE(0..32))
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The device name information of ONU."
        ::= { rcEponOnuCtcDevInfoEntry 12 }

