------------------------------------------------------------------------------
	rcL2AccessList	    	OBJECT IDENTIFIER ::= {	rcAclCfg 1 }
	rcIpv4AccessList		OBJECT IDENTIFIER ::= {	rcAclCfg 2 }
	rcIpv6AccessList	    OBJECT IDENTIFIER ::= {	rcAclCfg 3 }
	rcHybridAccessList      OBJECT IDENTIFIER ::= {	rcAclCfg 4 } 	
	rcUserAccessList        OBJECT IDENTIFIER ::= {	rcAclCfg 5 }
	rcAclAttribute          OBJECT IDENTIFIER ::= {	rcAclCfg 6 }  
	rcIPv6HybridAccessList  OBJECT IDENTIFIER ::= {	rcAclCfg 7 }
------------------------------------------------------------------------------
	  	
-- L2 Access List Control Table
	-- The l2 access control list contains some rules that are 
    -- associated with layer 2 and will be bound to the ports
    -- or vlans on device.

     rcL2AclTable    OBJECT-TYPE
        SYNTAX          SEQUENCE OF  RcL2AclEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Layer 2 access list control table"
    	::= { rcL2AccessList 1 }    
    
     rcL2AclEntry  OBJECT-TYPE
		SYNTAX          RcL2AclEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the rcL2AclTable."
		INDEX { rcAclId, rcAclRuleId }
		::= { rcL2AclTable 1 }
		
	RcL2AclEntry ::=	SEQUENCE { 
		rcAclId                     Integer32,
		rcAclRuleId                 Integer32,
		rcL2AclAccessType           AclAccessType,
		rcL2AclMatchFieldFlag       AclMatchField,
		rcL2AclSVlanId              AclVlanId,
		rcL2AclSVlanCos             AclVlanCos,
		rcL2AclCVlanId              AclVlanId,
		rcL2AclCVlanCos             AclVlanCos,
		rcL2AclSrcMacAddress        MacAddress,
		rcL2AclSrcMacMask           MacAddress,
		rcL2AclDstMacAddress        MacAddress,
		rcL2AclDstMacMask           MacAddress,
		rcL2AclEthernetType         INTEGER,
		rcL2AclEthernetTypeMask     INTEGER,
		rcL2AclRowStatus            RowStatus
		}		
    
		rcAclId  OBJECT-TYPE
	        SYNTAX 			Integer32(1..9000)
	        MAX-ACCESS 		read-only
	        STATUS 			current
	        DESCRIPTION		"Index of rcL2AclTable."
	        ::= { rcL2AclEntry 1 }        
         
    	rcAclRuleId  OBJECT-TYPE
	        SYNTAX 			Integer32(1..256)
	        MAX-ACCESS 		read-only
	        STATUS 			current
	        DESCRIPTION		"Index of L2 access control list rule."
	        ::= { rcL2AclEntry 2 }    
        
    	rcL2AclAccessType  OBJECT-TYPE
	        SYNTAX 			AclAccessType
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Access type of L2 acl rule."
	        ::= { rcL2AclEntry 3 }   
    
	    rcL2AclMatchFieldFlag  OBJECT-TYPE
	        SYNTAX 			AclMatchField
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Set the match filed of the table."
	        ::= { rcL2AclEntry 4 }  
	         
	    rcL2AclSVlanId  OBJECT-TYPE
	        SYNTAX 			AclVlanId
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"SVLAN ID."
	        ::= { rcL2AclEntry 5 }
	        
	    rcL2AclSVlanCos  OBJECT-TYPE
	        SYNTAX 			AclVlanCos    
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"SVLAN COS."
	        ::= { rcL2AclEntry 6 }
	        
	    rcL2AclCVlanId  OBJECT-TYPE
	        SYNTAX 			AclVlanId
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"CVLAN ID."
	        ::= { rcL2AclEntry 7 }
	        
	    rcL2AclCVlanCos  OBJECT-TYPE
	        SYNTAX 			AclVlanCos
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"CVLAN COS."
	        ::= { rcL2AclEntry 8 }
	        
	    rcL2AclSrcMacAddress  OBJECT-TYPE
	        SYNTAX 			MacAddress    
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Source MAC address."
	        ::= { rcL2AclEntry 9 }
	        
	    rcL2AclSrcMacMask  OBJECT-TYPE
	        SYNTAX 			MacAddress
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Sourcd MAC address mask."
	        ::= { rcL2AclEntry 10 }
	        
	    rcL2AclDstMacAddress  OBJECT-TYPE
	        SYNTAX 			MacAddress    
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Destination MAC address."
	        ::= { rcL2AclEntry 11 }
	        
	    rcL2AclDstMacMask  OBJECT-TYPE
	        SYNTAX 			MacAddress
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Destination MAC address mask."
	        ::= { rcL2AclEntry 12 }
	        
	    rcL2AclEthernetType  OBJECT-TYPE
	        SYNTAX 			INTEGER(1536..65535)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Ethernet type in layer 2 framer header."
	        ::= { rcL2AclEntry 13 }
	        
	    rcL2AclEthernetTypeMask  OBJECT-TYPE
	        SYNTAX 			INTEGER(0..65535)    
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Ethernet type mask."
	        ::= { rcL2AclEntry 14 }
	        
	    rcL2AclRowStatus  OBJECT-TYPE
	        SYNTAX 			RowStatus
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Row status of the table."
	        ::= { rcL2AclEntry 15 }


-- Ipv4 Access List Control Table
	-- The Ipv4 access control list contains some rules that are 
    -- associated with Ipv4 header and will be bound to the ports
    -- or vlans on device.

     rcIpv4AclTable    OBJECT-TYPE
        SYNTAX          SEQUENCE OF  RcIpv4AclEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "IP version 4 access list control table"
    	::= { rcIpv4AccessList 1 }    
    
     rcIpv4AclEntry  OBJECT-TYPE
		SYNTAX          RcIpv4AclEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the rcIpv4AclTable."
		INDEX { rcAclId, rcAclRuleId }
		::= { rcIpv4AclTable 1 }
		
	RcIpv4AclEntry ::=	SEQUENCE { 
		rcIpv4AclAccessType         AclAccessType,
		rcIpv4AclMatchFieldFlag     AclMatchField,
		rcIpv4AclSrcIpv4Address     IpAddress,
		rcIpv4AclSrcIpv4Mask        IpAddress,
		rcIpv4AclDestIpv4Address    IpAddress,
		rcIpv4AclDestIpv4Mask       IpAddress,
		rcIpv4AclIpv4Precedence     INTEGER,
		rcIpv4AclIpv4Tos            INTEGER,
		rcIpv4AclIpv4Dscp           INTEGER,
		rcIpv4AclIpv4MfFlag         INTEGER,
		rcIpv4AclUpperProtocol      INTEGER,
		rcIpv4AclSrcProtocolPort    InetPortNumber,
		rcIpv4AclDestProtocolPort   InetPortNumber,
		rcIpv4AclTcpFlags           INTEGER,
		rcIpv4AclRowStatus          RowStatus,
		rcIpv4AclSrcProtocolPortBoundHigh    InetPortNumber,
		rcIpv4AclDestProtocolPortBoundHigh   InetPortNumber
		}		

    	rcIpv4AclAccessType  OBJECT-TYPE
	        SYNTAX 			AclAccessType
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Access type of L2 acl rule."
	        ::= { rcIpv4AclEntry 1 }   
    
	    rcIpv4AclMatchFieldFlag  OBJECT-TYPE
	        SYNTAX 			AclMatchField
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Set the match filed of the table."
	        ::= { rcIpv4AclEntry 2 }  
	         
	    rcIpv4AclSrcIpv4Address  OBJECT-TYPE
	        SYNTAX 			IpAddress
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Source IP address."
	        ::= { rcIpv4AclEntry 3 }
	        
	    rcIpv4AclSrcIpv4Mask  OBJECT-TYPE
	        SYNTAX 			IpAddress    
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"SVLAN COS."
	        ::= { rcIpv4AclEntry 4 }
	        
	    rcIpv4AclDestIpv4Address  OBJECT-TYPE
	        SYNTAX 			IpAddress
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Source IP address mask."
	        ::= { rcIpv4AclEntry 5 }
	        
	    rcIpv4AclDestIpv4Mask  OBJECT-TYPE
	        SYNTAX 			IpAddress
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Destination IP address."
	        ::= { rcIpv4AclEntry 6 }
	        
	    rcIpv4AclIpv4Precedence  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					routine(0),
			                    priority(1),
			                    immediate(2),
			                    flash(3),
			                    flash-override(4),
			                    critical(5),
			                    internet(6),
			                    network(7)
			                }
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The value of the IP precedence in the IP protocol. 
							It conflicts with the Tos and Dscp when setting."
	        ::= { rcIpv4AclEntry 7 }
	        
	    rcIpv4AclIpv4Tos  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..15)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The value of the TOS in the IP protocol. 
							It conflicts with the Precedence and Dscp when setting."
	        ::= { rcIpv4AclEntry 8 }
	        
	    rcIpv4AclIpv4Dscp  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..63)    
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The value of the DSCP in the IP protocol. 
							It conflicts with the Precedence and Tos when setting."
	        ::= { rcIpv4AclEntry 9 }
	        
	    rcIpv4AclIpv4MfFlag  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					nofragments(1),
	        					fragments(2)
	        				}
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The flag that marks whether there are another IP fragments."
	        ::= { rcIpv4AclEntry 10 }
	        
	    rcIpv4AclUpperProtocol  OBJECT-TYPE
	        SYNTAX 			INTEGER(0..255)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The protocol type in the IP protocol.To match any Internet protocol, use the ip(0)."
	        ::= { rcIpv4AclEntry 11 }
	        
	    rcIpv4AclSrcProtocolPort  OBJECT-TYPE
	        SYNTAX 			InetPortNumber    
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The source protocol port in the TCP or UDP protocol."
	        ::= { rcIpv4AclEntry 12 }
	        
	    rcIpv4AclDestProtocolPort  OBJECT-TYPE
	        SYNTAX 			InetPortNumber
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The destination protocol port in the TCP or UDP protocol."
	        ::= { rcIpv4AclEntry 13 }
	        
	    rcIpv4AclTcpFlags  OBJECT-TYPE
	        SYNTAX 			INTEGER(0..63)    
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The flags in the TCP protocol."
	        ::= { rcIpv4AclEntry 14 }
	        
	    rcIpv4AclRowStatus  OBJECT-TYPE
	        SYNTAX 			RowStatus
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Row status of the table."
	        ::= { rcIpv4AclEntry 15 } 
	        
	    rcIpv4AclSrcProtocolPortBoundHigh  OBJECT-TYPE
	        SYNTAX 			InetPortNumber    
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The source protocol end port in the TCP or UDP protocol."
	        ::= { rcIpv4AclEntry 16 }
	        
	    rcIpv4AclDestProtocolPortBoundHigh  OBJECT-TYPE
	        SYNTAX 			InetPortNumber
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The destination protocol end port in the TCP or UDP protocol."
	        ::= { rcIpv4AclEntry 17 }

