--Textual Conventions
------------------------------------------------------------------------------
--	textual conventions in swtich-acl-mib
------------------------------------------------------------------------------
	AclAccessType ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION                
        		"Access type of ACL"
        SYNTAX       INTEGER { deny(1), permit(2) }
        
    AclMatchField ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION                
        		"Match filed of acl table"
        SYNTAX       OCTET STRING ( SIZE(8) )
        
    AclVlanId ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION                
        		"VLAN ID"
        SYNTAX       INTEGER( 1..4094 )
        
    AclVlanCos ::= TEXTUAL-CONVENTION
        STATUS       current
        DESCRIPTION                
        		"VLAN COS"
        SYNTAX       INTEGER( 0..7 )
------------------------------------------------------------------------------


