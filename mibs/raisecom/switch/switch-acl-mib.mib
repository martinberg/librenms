-- User Access List Control Table
	-- The user access control list profile
	rcUserAclProfileTable OBJECT-TYPE
		SYNTAX          SEQUENCE OF  RcUserAclProfileEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "User access list profile table"
    	::= { rcUserAccessList 1 }    
    
     rcUserAclProfileEntry  OBJECT-TYPE
		SYNTAX          RcUserAclProfileEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the rcUserAclTable."
		INDEX { rcUserAclFieldId }
		::= { rcUserAclProfileTable 1 }
		
	RcUserAclProfileEntry ::=	SEQUENCE { 
		rcUserAclFieldId           Integer32,
		rcUserAclFieldStart        INTEGER,
		rcUserAclFieldOffset       INTEGER,
		rcUserAclFieldLength       INTEGER,
		rcUserAclFieldReferTimes   Integer32,
		rcUserAclFieldRowStatus    RowStatus
		}
		  
	rcUserAclFieldId  OBJECT-TYPE
	        SYNTAX 			Integer32(1..8)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Match field ID."
	        ::= { rcUserAclProfileEntry 1 }
	        
	rcUserAclFieldStart  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					l2(1),
	        					l3(2),
	        					l4(3)
	        					}
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Match field start position."
	        ::= { rcUserAclProfileEntry 2 }
	        
	rcUserAclFieldOffset  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..123)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Offset of match field relative the start position."
	        ::= { rcUserAclProfileEntry 3 } 
	        
	rcUserAclFieldLength  OBJECT-TYPE
	        SYNTAX 			INTEGER
	        MAX-ACCESS 		read-only
	        STATUS 			current
	        DESCRIPTION		"Length of match field."
	        ::= { rcUserAclProfileEntry 4 }
	        
	rcUserAclFieldReferTimes  OBJECT-TYPE
	        SYNTAX 			Integer32
	        MAX-ACCESS 		read-only
	        STATUS 			current
	        DESCRIPTION		"Whether the match field is used."
	        ::= { rcUserAclProfileEntry 5 }
	        
	rcUserAclFieldRowStatus  OBJECT-TYPE
	        SYNTAX 			RowStatus
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Row status."
	        ::= { rcUserAclProfileEntry 6 }

	-- The user access control list contains some rules that are 
    -- associated with layer 2, Ipv4 header, Ipv6 header and others,
    -- and will be bound to the ports or vlans on device.

     rcUserAclTable    OBJECT-TYPE
        SYNTAX          SEQUENCE OF  RcUserAclEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "User access list control table"
    	::= { rcUserAccessList 2 }    
    
     rcUserAclEntry  OBJECT-TYPE
		SYNTAX          RcUserAclEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the rcUserAclTable."
		INDEX { rcAclId, rcAclRuleId }
		::= { rcUserAclTable 1 }
		
	RcUserAclEntry ::=	SEQUENCE { 
		rcUserAclAccessType           AclAccessType,
		rcUserAclFieldIdList          AclMatchField,
		rcUserAclMatchString          OCTET STRING,
		rcUserAclMatchMask            OCTET STRING,
		rcUserAclMatchTagType         INTEGER,
		rcUserAclRowStatus            RowStatus
		}		
        
    	rcUserAclAccessType  OBJECT-TYPE
	        SYNTAX 			AclAccessType
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Access type of L2 acl rule."
	        ::= { rcUserAclEntry 1 }   
    
	    rcUserAclFieldIdList  OBJECT-TYPE
	        SYNTAX 			AclMatchField
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Set the match filed id list of the table."
	        ::= { rcUserAclEntry 2 }  
	         
	    rcUserAclMatchString  OBJECT-TYPE
	        SYNTAX 			OCTET STRING (SIZE (0..32))
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Match string."
	        ::= { rcUserAclEntry 3 }
	        
	    rcUserAclMatchMask  OBJECT-TYPE
	        SYNTAX 			OCTET STRING (SIZE (0..32))
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Match field mask."
	        ::= { rcUserAclEntry 4 }
	        
	    rcUserAclMatchTagType  OBJECT-TYPE
	        SYNTAX 			INTEGER{
	        					any(1),
	        					untagged(2),
	        					single-tagged(3),
	        					double-tagged(4)
	        					}
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Match packet type."
	        ::= { rcUserAclEntry 5 }
	        
	    rcUserAclRowStatus  OBJECT-TYPE
	        SYNTAX 			RowStatus
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Row status."
	        ::= { rcUserAclEntry 6 }
	        
