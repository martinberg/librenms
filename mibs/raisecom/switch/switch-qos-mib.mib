--
-- QOS action police profile config
--

--rcQosPoliceProfileScalar
--	rcQosPoliceProfileScalar    OBJECT IDENTIFIER::= { rcQosPoliceProfile 1 }    
    
    rcQosPoliceProfileNextId  OBJECT-TYPE
		SYNTAX          INTEGER(1..1024)
		MAX-ACCESS      read-only
        STATUS          current
		DESCRIPTION     "Next id of police profile."
		::= { rcQosPoliceProfile 1 }


--rcQosPoliceProfileTable
	rcQosPoliceProfileTable    OBJECT-TYPE
        SYNTAX          SEQUENCE OF  RcQosPoliceProfileEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Police profile table"
    	::= { rcQosPoliceProfile 2 }    
    
    rcQosPoliceProfileEntry  OBJECT-TYPE
		SYNTAX          RcQosPoliceProfileEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the rcQosPoliceProfileTable."
		INDEX { rcQosPoliceProfileId }
		::= { rcQosPoliceProfileTable 1 }
		
	RcQosPoliceProfileEntry ::=	SEQUENCE { 
		rcQosPoliceProfileId                 INTEGER,
		rcQosPoliceProfileType               INTEGER,
		rcQosPoliceProfileRate               Unsigned32,
		rcQosPoliceProfileBurstSize          Unsigned32,
		rcQosPoliceProfileExccedAction       INTEGER,
		rcQosPoliceProfileMarkDown           INTEGER,
		rcQosPoliceProfileReferTimes         INTEGER,
		rcQosPoliceProfileRowStatus          RowStatus,
		rcQosPoliceProfileBucketType         INTEGER,
		rcQosPoliceProfileCir                Unsigned32,
		rcQosPoliceProfileCbs                Unsigned32,
		rcQosPoliceProfilePir                Unsigned32,
		rcQosPoliceProfilePbs                Unsigned32,
		rcQosPoliceProfilePktColorAware      TruthValue
		
		}
    
    rcQosPoliceProfileId  OBJECT-TYPE
	        SYNTAX 			INTEGER (1..1024)
	        MAX-ACCESS 		not-accessible
	        STATUS 			current
	        DESCRIPTION		"Police profile ID."
	        ::= { rcQosPoliceProfileEntry 1 }
	        
	rcQosPoliceProfileType  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					single(1),
	        					aggregate(2)
	        				}
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Police profile type."
	        ::= { rcQosPoliceProfileEntry 2 }

	rcQosPoliceProfileRate  OBJECT-TYPE
	        SYNTAX 			Unsigned32 (1..1048576)
	        UNITS           "Mbps"
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Token rate, It is specified in units of Mbps. 
            A rate of zero means that all packets will be out of profile.
            If the qosPolicerAction is set to drop then this effectively
            denies any service to packets policed by this policer."
	        ::= { rcQosPoliceProfileEntry 3 }
	        
	rcQosPoliceProfileBurstSize  OBJECT-TYPE
	        SYNTAX 			Unsigned32 (1..512)
	        UNITS           "Kbps"
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Burst rate, The normal size of a burst in terms of kB"
	        ::= { rcQosPoliceProfileEntry 4 }

	rcQosPoliceProfileExccedAction  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					drop(1),
	        					policed-dscp(2)
	        				}
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Indication of how to handle out of profile packet."
	        ::= { rcQosPoliceProfileEntry 5 }

	rcQosPoliceProfileMarkDown  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..63)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"The maker down dscp value of policer when the traffic is out profile."
	        ::= { rcQosPoliceProfileEntry 6 }

	rcQosPoliceProfileReferTimes OBJECT-TYPE
			SYNTAX			INTEGER
			MAX-ACCESS      read-only
			STATUS          current
			DESCRIPTION     "Times the meter profile is refered."
			::= { rcQosPoliceProfileEntry 7 }
	        
	rcQosPoliceProfileRowStatus  OBJECT-TYPE
	        SYNTAX 			RowStatus
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Row status."
	        ::= { rcQosPoliceProfileEntry 8 }

	rcQosPoliceProfileBucketType  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					single-bucket(1),
	        					double-bucket(2)
	        				}
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Bucket type of policer profile, namely single bucket(C bucket) or double bucket(C bucket and E bucket)."
	        ::= { rcQosPoliceProfileEntry 9 }
	        
	rcQosPoliceProfileCir  OBJECT-TYPE
	        SYNTAX 			Unsigned32
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Committed information rate of C bucket."
	        ::= { rcQosPoliceProfileEntry 10 }
	        
	 rcQosPoliceProfileCbs  OBJECT-TYPE
	        SYNTAX 			Unsigned32
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Committed burst size of C bucket."
	        ::= { rcQosPoliceProfileEntry 11 }
	        
	 rcQosPoliceProfilePir  OBJECT-TYPE
	        SYNTAX 			Unsigned32
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Committed information rate of E bucket."
	        ::= { rcQosPoliceProfileEntry 12 }
	        
	rcQosPoliceProfilePbs  OBJECT-TYPE
	        SYNTAX 			Unsigned32
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Committed burst size of E bucket."
	        ::= { rcQosPoliceProfileEntry 13 }
	        
     rcQosPoliceProfilePktColorAware  OBJECT-TYPE
	        SYNTAX 			TruthValue
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Check whether color in original pakcet is aware of."
	        ::= { rcQosPoliceProfileEntry 14 }

--rcQosPoliceProfilePacketActionTable
	rcQosPoliceProfilePacketActionTable    OBJECT-TYPE
        SYNTAX          SEQUENCE OF  RcQosPoliceProfilePacketActionEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Police profile packet action table"
    	::= { rcQosPoliceProfile 3 }    
    
    rcQosPoliceProfilePacketActionEntry  OBJECT-TYPE
		SYNTAX          RcQosPoliceProfilePacketActionEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the rcQosPoliceProfilePacketActionTable."
		INDEX { rcQosPoliceProfileId, rcQosPoliceProfilePacketColor }
		::= { rcQosPoliceProfilePacketActionTable 1 }
		
	RcQosPoliceProfilePacketActionEntry ::=	SEQUENCE { 
		rcQosPoliceProfilePacketColor           INTEGER,
		rcQosPoliceProfilePacketAction          BITS,
		rcQosPoliceProfilePacketRemarkedCos     INTEGER,
		rcQosPoliceProfilePacketRemarkedDscp    INTEGER,
		rcQosPoliceProfilePacketRecolored       INTEGER
		}
    
    rcQosPoliceProfilePacketColor  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					red(1),
	        					green(2),
	        					yellow(3)
	        					}
	        MAX-ACCESS 		not-accessible
	        STATUS 			current
	        DESCRIPTION		"Packet color."
	        ::= { rcQosPoliceProfilePacketActionEntry 1 }
	        
	rcQosPoliceProfilePacketAction  OBJECT-TYPE
	        SYNTAX 			BITS {
	        					none(0),
	        					drop(1),
	        					remark-cos(2),
	        					remark-dscp(3),
	        					recolor(4)
	        					}
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Action of red packet."
	        ::= { rcQosPoliceProfilePacketActionEntry 2 }
	        
	 rcQosPoliceProfilePacketRemarkedCos  OBJECT-TYPE
	        SYNTAX 			INTEGER(0..7)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"COS value that red packet is remarked."
	        ::= { rcQosPoliceProfilePacketActionEntry 3 }
	        
	rcQosPoliceProfilePacketRemarkedDscp  OBJECT-TYPE
	        SYNTAX 			INTEGER(0..63)
	        MAX-ACCESS 		read-create
	        STATUS 			current
