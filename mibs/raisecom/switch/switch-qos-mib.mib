--	  	
-- Qos port config
--

--rcQosPortCfgTable	
	rcQosPortCfgTable    OBJECT-TYPE
        SYNTAX          SEQUENCE OF  RcQosPortCfgEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Qos port config table"
    	::= { rcQosPort 1 }    
    
     rcQosPortCfgEntry  OBJECT-TYPE
		SYNTAX          RcQosPortCfgEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the rcQosPortCfgTable."
		INDEX { ifIndex }
		::= { rcQosPortCfgTable 1 }
		
	RcQosPortCfgEntry ::=	SEQUENCE { 
		rcQosPortCfgTrustMode       INTEGER,
		rcQosPortCfgQueueScheduler  INTEGER,
		rcQosPortCfgDefaultCos      INTEGER,
		rcQosPortCfgCosOverride     EnableVar,
		rcQosPortCfgDefaultDscp     INTEGER,
		rcQosPortCfgDscpOverride    EnableVar,
		rcQosPortCfgVlanPriOverride INTEGER
		}		
    
		rcQosPortCfgTrustMode  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					cos(1),
	        					dscp(2),
	        					tos(3),
	        					trafficclass(4)
	        				}
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Index of rcQosPortCfgTable."
	        ::= { rcQosPortCfgEntry 1 }        
         
    	rcQosPortCfgQueueScheduler  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					sp(1),
	        					wdrr(2),
	        					wrr(3)
	        				}
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Index of L2 access control list rule."
	        ::= { rcQosPortCfgEntry 2 }    
        
    	rcQosPortCfgDefaultCos  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..7)
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Qos port default cos value."
	        ::= { rcQosPortCfgEntry 3 }   
    
	    rcQosPortCfgCosOverride  OBJECT-TYPE
	        SYNTAX 			EnableVar
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Enable of disable port cos override function."
	        ::= { rcQosPortCfgEntry 4 }  
	         
	    rcQosPortCfgDefaultDscp  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..63)
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Qos port default dscp value."
	        ::= { rcQosPortCfgEntry 5 }
	        
	    rcQosPortCfgDscpOverride  OBJECT-TYPE
	        SYNTAX 			EnableVar    
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Enable or disable port dscp override function."
	        ::= { rcQosPortCfgEntry 6 }
	        
	    rcQosPortCfgVlanPriOverride  OBJECT-TYPE
	    	SYNTAX          INTEGER {
	    						null(0),
	    						frame-priority(1),
	    						flow-priority(2),
	    						both(3)
	    						}
	    	MAX-ACCESS      read-write
	    	STATUS          current
	    	DESCRIPTION     "Indicates whether VLANpriority override feature on the port."
	    	::= { rcQosPortCfgEntry 7 }
	        
	        
-- rcQosPortSchedulerQueueTable	        
