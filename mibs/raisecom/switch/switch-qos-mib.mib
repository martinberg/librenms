--rcQosPortQueueWredTable
	rcQosPortQueueWredTable    OBJECT-TYPE
        SYNTAX          SEQUENCE OF  RcQosPortQueueWredEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION     "Port queue congestion avoidance config table"
    	::= { rcQosPort 5 }    
    
    rcQosPortQueueWredEntry  OBJECT-TYPE
		SYNTAX          RcQosPortQueueWredEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the rcQosPortQueueWredTable."
		INDEX { ifIndex, rcQosPortSchedulerQueueId, rcQosPortQueuePacketColor }
		::= { rcQosPortQueueWredTable 1 }
		
	RcQosPortQueueWredEntry ::=	SEQUENCE { 
		rcQosPortQueuePacketColor              INTEGER,
		rcQosPortQueuePacketLowLimit           INTEGER,
		rcQosPortQueuePacketHighLimit          INTEGER,
		rcQosPortQueuePacketDropProbability    INTEGER
		}
    
    rcQosPortQueuePacketColor  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					red(1),
	        					green(2),
	        					yellow(3)
	        					}
	        MAX-ACCESS 		not-accessible
	        STATUS 			current
	        DESCRIPTION		"Packet color index, namely red, green and yellow packet."
	        ::= { rcQosPortQueueWredEntry 1 }
	
	rcQosPortQueuePacketLowLimit  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..100)
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Low limit ratio that red packet begin to drop."
	        ::= { rcQosPortQueueWredEntry 2 }
	        
	rcQosPortQueuePacketHighLimit  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..100)
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"High limit ratio that red packet begin to drop."
	        ::= { rcQosPortQueueWredEntry 3 }
	        
	rcQosPortQueuePacketDropProbability  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..100)
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Drop probability of packet colored to be red."
	        ::= { rcQosPortQueueWredEntry 4 }
	        
