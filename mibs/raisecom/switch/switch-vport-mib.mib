-- VPort administration management table
	 
	 roapVPortAdminTable OBJECT-TYPE
	 	SYNTAX      SEQUENCE OF RoapVPortAdminEntry
	 	MAX-ACCESS      not-accessible
        STATUS          current
	 	DESCRIPTION     "A table of	virtual port administration."
	 	::=	{ roapVPortCfgMib 1 }
	 	       
     roapVPortAdminEntry  OBJECT-TYPE
		SYNTAX          RoapVPortAdminEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the roapVPortAdminTable."
		INDEX { roapPonPortIndex }
		::= { roapVPortAdminTable 1 }
		
	RoapVPortAdminEntry ::=	SEQUENCE {
		roapPonPortIndex           OltPortIndex,
		roapVPortAdmin             EnableVar
		}		
        
        roapPonPortIndex  OBJECT-TYPE
	        SYNTAX 			OltPortIndex
	        MAX-ACCESS 		not-accessible
	        STATUS 			current
	        DESCRIPTION		"Index of roapVPortAdminTable, mainly refer to EPON port."
	        ::= { roapVPortAdminEntry 1 }
        
		roapVPortAdmin  OBJECT-TYPE
	        SYNTAX 			EnableVar
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Administrate virtual port function on EPON port."
	        ::= { roapVPortAdminEntry 2 }        
         
    	
-- VLAN attribute administration on VPort

	roapVPortVlanTable OBJECT-TYPE
	 	SYNTAX      SEQUENCE OF RoapVPortVlanEntry
	 	MAX-ACCESS      not-accessible
        STATUS          current
	 	DESCRIPTION     "A table of	VPort VLAN attribute administration."
	 	::=	{ roapVPortCfgMib 2 }

     roapVPortVlanEntry  OBJECT-TYPE
		SYNTAX          RoapVPortVlanEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the roapVPortVlanTable."
		INDEX { roapVPortIndex }
		::= { roapVPortVlanTable 1 }
		
	RoapVPortVlanEntry ::=	SEQUENCE {
		roapVPortIndex              OltPortIndex, 
		roapVPortNativeVlanId       INTEGER,
		roapVPortAllowedVlanList    Vlanset,
		roapVPortUntaggedVlanList   Vlanset,
		roapVportBasedQinQAdmin     EnableVar,
		roapVportVlanMode           INTEGER
		}		
        
        roapVPortIndex  OBJECT-TYPE
	        SYNTAX 			OltPortIndex
	        MAX-ACCESS 		not-accessible
	        STATUS 			current
	        DESCRIPTION		"Virtual port Index."
	        ::= { roapVPortVlanEntry 1 }

        roapVPortNativeVlanId  OBJECT-TYPE
	        SYNTAX 			INTEGER (1..4094)
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Native VLAN ID of virtual port."
	        ::= { roapVPortVlanEntry 2 }
	        
	    roapVPortAllowedVlanList  OBJECT-TYPE
	        SYNTAX 			Vlanset
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"VLAN list that are allowed to pass the virtual port."
	        ::= { roapVPortVlanEntry 3 } 
	        
	    roapVPortUntaggedVlanList  OBJECT-TYPE
	        SYNTAX 			Vlanset
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Untagged VLAN list in egress of virtual port."
	        ::= { roapVPortVlanEntry 4 } 
	        
	    roapVportBasedQinQAdmin  OBJECT-TYPE
	        SYNTAX 			EnableVar
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"Basic QinQ function of virtual port."
	        ::= { roapVPortVlanEntry 5 }
	        
	    roapVportVlanMode  OBJECT-TYPE
	    	SYNTAX 			INTEGER {
	    						transparent(1),
	    						trunk(2)
	    						}
	        MAX-ACCESS 		read-write
	        STATUS 			current
	        DESCRIPTION		"VLAN mode of virtual port."
	        ::= { roapVPortVlanEntry 6 }

-- VLAN aggregation administration on VPort

	roapVPortVARuleTable OBJECT-TYPE
	 	SYNTAX      SEQUENCE OF RoapVPortVARuleEntry
	 	MAX-ACCESS      not-accessible
        STATUS          current
	 	DESCRIPTION     "A table of	VPort VLAN attribute administration."
