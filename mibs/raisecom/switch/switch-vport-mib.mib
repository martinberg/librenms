     roapVPortVARuleEntry  OBJECT-TYPE
		SYNTAX          RoapVPortVARuleEntry
		MAX-ACCESS      not-accessible
        STATUS          current
		DESCRIPTION     "An entry in the roapVPortVARuleTable."
		INDEX { roapVPortIndex, roapVPortVACVlanId }
		::= { roapVPortVARuleTable 1 }
		
	RoapVPortVARuleEntry ::=	SEQUENCE {
		roapVPortVACVlanId           INTEGER, 
		roapVPortVACVlanAction       INTEGER,
		roapVPortVANewOVID           INTEGER,
		roapVPortVANewOVlanCos       INTEGER,
		roapVPortVANewIVID           INTEGER,
		roapVPortVANewIVlanCos       INTEGER,
		roapVPortVARowStatus         RowStatus
		}		
        
        roapVPortVACVlanId  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..4094)
	        MAX-ACCESS 		not-accessible
	        STATUS 			current
	        DESCRIPTION		"CVLAN ID."
	        ::= { roapVPortVARuleEntry 1 }
	        
	    roapVPortVACVlanAction  OBJECT-TYPE
	        SYNTAX 			INTEGER {
	        					unchange(1),
	        					add(2),
	        					remove(3),
	        					change(4)
	        					}
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Inner vlan aggregation action, copyfromouter action is specially used for VLAN range aggregation."
	        ::= { roapVPortVARuleEntry 2 }
	        
	    roapVPortVANewOVID  OBJECT-TYPE
	        SYNTAX 			INTEGER (1..4094)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Outer VLAN ID after aggregation."
	        ::= { roapVPortVARuleEntry 3 }
	        
	    roapVPortVANewOVlanCos  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..7)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Outer VLAN COS after aggregation."
	        ::= { roapVPortVARuleEntry 4 }
	        
	    roapVPortVANewIVID  OBJECT-TYPE
	        SYNTAX 			INTEGER (1..4094)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Inner VLAN ID after aggregation."
	        ::= { roapVPortVARuleEntry 5 }
	        
	    roapVPortVANewIVlanCos  OBJECT-TYPE
	        SYNTAX 			INTEGER (0..7)
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Inner VLAN COS after aggregation."
	        ::= { roapVPortVARuleEntry 6 }
	        
	    roapVPortVARowStatus  OBJECT-TYPE
	        SYNTAX 			RowStatus
	        MAX-ACCESS 		read-create
	        STATUS 			current
	        DESCRIPTION		"Row status."
	        ::= { roapVPortVARuleEntry 7 }

